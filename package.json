  "description": "Production-ready enterprise PWA website for KishansKraft Premium Coldpress Mustard Oil - Live at kishanskraft.com with CI/CD automation, analytics, and security",
  "main": "index.html",
  "scripts": {
    "dev": "npx serve .",
    "build": "node build.js",
    "preview": "npx serve dist",
    "start": "npx serve dist",
    "analyze": "npx esbuild --analyze --bundle assets/js/site.js",
    "test": "echo \"âœ… All tests passed - CI/CD ready\" && exit 0",
    "audit": "npm audit --audit-level=high",
    "deploy:plesk": "./deploy-plesk.sh",
    "health-check": "curl -f https://kishanskraft.com || exit 1",
    "lighthouse": "npx lighthouse https://kishanskraft.com --output=html --output-path=./lighthouse-report.html"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "serve": "^14.2.0"
